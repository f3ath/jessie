{
  "tests": [
    {
      "selector": "$[?((@ > 2) || (@ < -.14))]",
      "document": [-100, 0, 1, 2, 3, 3.14, 4, "4", "1", true, [], {}],
      "values": [-100, 3, 3.14, 4]
    }, {
      "selector": "$[?((@ < 2) && @)]",
      "document": [-100, 0, 1, 2, 3, 3.14, 4, "4", "1", true, [], {}],
      "values": [-100, 1]
    }, {
      "selector": "$[?((@ > 2.5) || (@ >= '1'))]",
      "document": [-100, 0, 1, 2, 3, 3.14, 4, "4", "1", "foo", true, [], {}, false],
      "values": [3, 3.14, 4, "4", "1", "foo"]
    }, {
      "selector": "$[?(1 < 2)]",
      "document": [-100, 0, 1, 2, 3, 3.14, 4, "4", "1", "foo", true, [], {}, false],
      "values": [-100, 0, 1, 2, 3, 3.14, 4, "4", "1", "foo", true, [], {}, false]
    }, {
      "name": "all truthy",
      "selector": "$[?(@)]",
      "document": [-42, -2.718, 0, 3.14, 4, "", "4", "0", "foo", true, [], {}, false],
      "values": [-42, -2.718, 3.14, 4, "4", "0", "foo", true, [], {}]
    }, {
      "name": "all falsy",
      "selector": "$[?(!@)]",
      "document": [-42, -2.718, 0, 3.14, 4, "", "4", "0", "foo", true, [], {}, false],
      "values": [0, "", false]
    }
  ]
}