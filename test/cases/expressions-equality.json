{
  "tests": [
    {
      "name": "tautology means true",
      "selector": "$[?(1==1)]",
      "document": [42],
      "values": [42],
      "paths": ["$[0]"],
      "pointers": ["/0"]
    }, {
      "name": "tautology in parens means true",
      "selector": "$[?((1==1) == (2==2))]",
      "document": [42],
      "values": [42],
      "paths": ["$[0]"],
      "pointers": ["/0"]
    }, {
      "name": "object itself",
      "selector": "$[?(@ == 1)]",
      "document": [0, 1, 2, "1", "a", "0", ""],
      "values": [1]
    }, {
      "name": "array index",
      "selector": "$[?(@[1] == 'b')]",
      "document": [["a", "b"], ["x", "y"]],
      "values": [["a", "b"]],
      "paths": ["$[0]"],
      "pointers": ["/0"]
    }, {
      "name": "object child, single quote",
      "selector": "$[?(@['key'] == 'b')]",
      "document": [{"key": "a"}, {"key": "b"}, {}],
      "values": [{"key": "b"}],
      "paths": ["$[1]"],
      "pointers": ["/1"]
    }, {
      "name": "object child, double quote",
      "selector": "$[?(@[\"key\"] == 'b')]",
      "document": [{"key": "a"}, {"key": "b"}, {}],
      "values": [{"key": "b"}],
      "paths": ["$[1]"],
      "pointers": ["/1"]
    }, {
      "name": "object child, dot-notation",
      "selector": "$[?(@.key=='b')]",
      "document": [{"key": "a"}, {"key": "b"}, {}],
      "values": [{"key": "b"}],
      "paths": ["$[1]"],
      "pointers": ["/1"]
    }, {
      "name": "object child, dot-notation, depth 2",
      "selector": "$[?(@.foo.bar=='b')]",
      "document": [{"foo": {"bar": "a"}}, {"foo": {"bar": "b"}}, {"foo": "b"} ,{}],
      "values": [{"foo": {"bar": "b"}}],
      "paths": ["$[1]"],
      "pointers": ["/1"]
    }, {
      "name": "object child, dot-notation, int value",
      "selector": "$[?(@.id==42)].name",
      "document": [{"id": 42, "name": "forty-two"}, {"id": 1, "name": "one"}],
      "values": ["forty-two"],
      "paths": ["$[0]['name']"],
      "pointers": ["/0/name"]
    }, {
      "name": "object child, combined",
      "selector": "$[?(@.foo['bar'] == 'b')]",
      "document": [{"foo": {"bar": "a"}}, {"foo": {"bar": "b"}}, {"foo": {"moo": 42}}, {}],
      "values": [{"foo": {"bar": "b"}}],
      "paths": ["$[1]"],
      "pointers": ["/1"]
    }, {
      "name": "equal props",
      "selector": "$[?(@.foo == @.bar)]",
      "document": [{"foo": 1, "bar": 2}, {"foo": 42, "bar": 42}, {"foo": 1, "bro": 1}, {}],
      "values": [{"foo": 42, "bar": 42}, {}],
      "paths": ["$[1]", "$[3]"],
      "pointers": ["/1", "/3"]
    }, {
      "selector": "$[?(1!=1)]",
      "document": [42],
      "values": []
    }, {
      "selector": "$[?((1==1) == (2==2))]",
      "document": [42],
      "values": [42]
    }, {
      "selector": "$[?(@ != 1)]",
      "document": [0, 1, 2, "1", "a", "0", ""],
      "values": [0, 2, "1", "a", "0", ""]
    }, {
      "selector": "$[?(@ == -1)]",
      "document": [0, -1, 2, "1", "a", "0", ""],
      "values": [-1]
    }, {
      "selector": "$[?(@ == 0)]",
      "document": [0, -1, 2, "1", "a", "0", ""],
      "values": [0]
    }, {
      "selector": "$[?(@ != '1')]",
      "document": [0, 1, 2, "1", "a", "0", ""],
      "values": [0, 1, 2, "a", "0", ""]
    }, {
      "selector": "$[?(@.foo != @.bar)]",
      "document": [{"foo": 1, "bar": 2}, {"foo": 42, "bar": 42}, {"foo": 1, "bro": 1}, {}],
      "values": [{"foo": 1, "bar": 2},  {"foo": 1, "bro": 1}]
    }
  ]
}