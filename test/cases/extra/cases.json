{
  "tests": [
    {
      "name": "reverse()",
      "selector" : "$[?reverse(@)=='cba']",
      "document" : ["abc", "cba"],
      "result": ["abc"]
    },
    {
      "name": "reverse(reverse())",
      "selector" : "$[?reverse(reverse(@))=='cba']",
      "document" : ["abc", "cba"],
      "result": ["cba"]
    },
    {
      "name": "reverse(reverse())",
      "selector" : "$..[?count(siblings(@)) == 1]",
      "document" : {"a":  {"b":  "x", "d":  "x"}},
      "result": ["x", "x"]
    }
  ]
}